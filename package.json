{
  "name": "openlmis-stockmanagement",
  "version": "0.0.1",
  "devDependencies": {
    "api-spec-converter": "~1.9.0",
    "raml2html": "~2.4.0",
    "raml-to-swagger": "1.0.1"
  },
  "scripts": {
    "runApiSpecConverter": "api-spec-converter --from raml --to swagger_2 ./build/resources/main/api-definition-raml.yaml > ./build/resources/main/static/stockmanagement/docs/api-definition.json",
    "runApiHtmlConverter": "raml2html --input ./build/resources/main/api-definition-raml.yaml --output ./build/resources/main/api-definition.html"
  }
}
